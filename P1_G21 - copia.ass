DATA: 
  i 0
  inicio1 #0A
  inicio2 #0B
  n 3
  pp 0
  b0 0
CODE:
inicio: 
  MOV B,(inicio1)
  MOV A,(B)
  MOV B,(inicio2)
  MOV B,(B)		
  CMP A,0
  JEQ end
  MOV (i),A
  MOV (b0),B
  MOV A,B
mult: 
  MOV B,A
  MOV A,(i)
  SUB A,1
  JEQ move
  MOV (i),A
  MOV A,B
  ADD A,(b0)
  JMP mult
move:	
  MOV B,(pp)
  ADD A,B
  MOV (pp),A
  MOV B,(inicio1)
  INC B
  MOV (inicio1),B
  MOV B,(inicio2)
  INC B
  MOV (inicio2),B
  MOV A,n
  SUB A,1
  JEQ end
  MOV (n),A
  JMP inicio
end:		
